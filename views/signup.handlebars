<div class="card">
    <div class="card-body">
        <h3 class="card-header">Sign Up</h3>
        <form id="signup-form">
            <div class="form-group">
                
                <input type="text" class="form-control" id="username-signup" autofocus="true" 
                    aria-describedby="username">
            </div>
            <div class="form-group">
               
                <input type="password" class="form-control" autofocus="true" id="password-signup">
            </div>
            <button type="submit" class="signupBtn">Signup!</button>
        </form>

    </div>
</div>


<script src="/js/signup.js"></script>